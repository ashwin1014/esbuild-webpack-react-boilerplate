{
  "name": "webpack-esbuild-react-boilerplate",
  "version": "1.0.0",
  "main": "src/index.js",
  "license": "MIT",
  "private": false,
  "author": "Ashwin Bordoloi",
  "contributors": [
    {
      "name": "Ashwin Bordoloi",
      "email": "ashwin.bordoloi@gmail.com"
    }
  ],
  "homepage": "https://github.com/ashwin1014/esbuild-webpack-react-boilerplate",
  "keywords": [
    "webpack",
    "boilerplate",
    "template",
    "esbuild-loader",
    "dart-sass",
    "styled-components",
    "react-webpack5-esbuild"
  ],
  "target": "web",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack serve --hot --mode development",
    "build": "cross-env NODE_ENV=production && webpack --mode production",
    "clean": "rimraf node_modules build yarn.lock package-lock.json",
    "clean-install": "yarn clean && yarn install",
    "clean-install-start": "yarn clean-install && yarn start",
    "clean-install-build": "yarn clean-install && yarn build",
    "prepare": "husky install",
    "svgr": "svgr -d src/assets/icons src/assets/icons/svgs --template src/assets/icons/template.js --memo --ignore-existing",
    "sass": "sass src/Sass:src/Css --watch --no-source-map",
    "eslint-check": "eslint --print-config src/index.js | eslint-config-prettier-check",
    "format": "prettier --write \"**/*.+(js|jsx|json|yml|yaml|css|md|vue)\"",
    "lint:scss": "stylelint **/*.scss",
    "lint:scss:fix": "sass-lint-auto-fix --config-sass-lint .sass-lint.yml",
    "lint:js": "eslint . --ignore-path .gitignore --ext .js,.jsx",
    "lint:js:fix": "npm run lint:js -- --fix",
    "pre-commit": "lint-staged",
    "generate": "plop",
    "check-updates": "ncu",
    "upgrade-packages": "yarn upgrade-interactive --latest",
    "postbuild": "react-snap"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ashwin1014/esbuild-webpack-react-boilerplate"
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "eslint --fix",
      "pretty-quick --staged"
    ],
    "src/**/*.scss": [
      "npm run lint:scss:fix",
      "stylelint **/*.scss --fix"
    ]
  },
  "reactSnap": {
    "include": [
      "/settings"
    ],
    "inlineCss": true,
    "headless": true,
    "cacheAjaxRequests": true
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/eslint-parser": "^7.15.7",
    "@babel/eslint-plugin": "^7.14.5",
    "@babel/plugin-proposal-optional-chaining": "^7.14.5",
    "@babel/plugin-proposal-private-methods": "^7.14.5",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-react-jsx": "^7.14.9",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-react": "^7.14.5",
    "@loadable/babel-plugin": "^5.13.2",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.1",
    "@svgr/cli": "^5.5.0",
    "autoprefixer": "^10.3.7",
    "babel-loader": "^8.2.2",
    "babel-plugin-import": "^1.13.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-named-asset-import": "^0.3.7",
    "babel-plugin-styled-components": "^1.13.2",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "babel-plugin-transform-remove-debugger": "^6.9.4",
    "chalk": "^4.1.2",
    "circular-dependency-plugin": "^5.2.2",
    "clean-webpack-plugin": "^4.0.0",
    "compression-webpack-plugin": "9.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.4",
    "dotenv-webpack": "^7.0.3",
    "esbuild-loader": "^2.15.1",
    "eslint": "7.2.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-module-resolver": "^1.2.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-webpack-plugin": "^2.5.4",
    "express": "^4.17.1",
    "fibers": "^5.0.0",
    "file-loader": "^6.2.0",
    "flex-gap-polyfill": "^4.1.0",
    "html-webpack-plugin": "^5.3.2",
    "husky": "7.0.2",
    "image-minimizer-webpack-plugin": "^2.2.0",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-jpegtran": "^7.0.0",
    "imagemin-mozjpeg": "^9.0.0",
    "imagemin-optipng": "^8.0.0",
    "imagemin-svgo": "9.0.0",
    "less-loader": "10.0.1",
    "lint-staged": "^11.2.0",
